{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "main": "main.js",
  "description": "App that controls access to the events of SPbSO",
  "author": "Alexey Naiden <alexey.nayden@gmail.com>",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear",
    "app": "electron .",
    "dist:mac": "electron-builder --dir --mac --config electron-builder.yml",
    "dist:win": "electron-builder --dir --win --config electron-builder.yml",
    "app-dev": "concurrently \"npm:dev\" \"npm:app\""
  },
  "dependencies": {
    "@carbon/colors": "^10.36.0",
    "electron-is-dev": "^2.0.0",
    "electron-reload": "^2.0.0-alpha.1",
    "postcss-preset-env": "^7.4.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "atcss": "^2.0.0",
    "autoprefixer": "^10.4.2",
    "carbon-components-svelte": "^0.59.0",
    "carbon-icons-svelte": "^10.44.4",
    "carbon-preprocess-svelte": "^0.6.0",
    "concurrently": "^7.0.0",
    "electron": "^17.1.0",
    "electron-builder": "^22.14.13",
    "node-sass": "^7.0.1",
    "postcss": "^8.4.5",
    "postcss-annotation-include": "^1.0.0",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.0.2",
    "postcss-load-config": "^3.1.1",
    "postcss-short": "^5.0.0",
    "postcss-simple-vars": "^6.0.3",
    "postcss-utilities": "^0.8.4",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sass": "^1.49.8",
    "sirv-cli": "^2.0.2",
    "stylelint": "^14.5.3",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "svelte": "^3.0.0",
    "svelte-preprocess": "^4.10.4",
    "svelte-preprocess-sass": "^2.0.1"
  }
}
